<picture>
    <source srcset="{{ baseURL }}-lg.jpg, {{ baseURL }}-lg-2x.jpg 2x" media="(min-width: 1200px)">
    <source srcset="{{ baseURL }}-md.jpg, {{ baseURL }}-md-2x.jpg 2x" media="(min-width: 992px)">
    <source srcset="{{ baseURL }}-sm.jpg, {{ baseURL }}-sm-2x.jpg 2x" media="(min-width: 385px)">
    <img srcset="{{ baseURL }}-xs.jpg, {{ baseURL }}-xs-2x.jpg 2x" alt="{{ alt }}">
</picture>
